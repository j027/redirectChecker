{
  "name": "discordbot",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start:dev": "tsx src/discordBot.ts",
    "start": "node dist/discordBot.js",
    "deploy:dev": "tsx src/deployCommands.ts",
    "deploy": "node dist/deployCommands.js",
    "init-db:dev": "tsx src/initDb.ts",
    "init-db": "node dist/initDb.js",
    "clean": "rm -rf dist",
    "prebuild": "yarn clean",
    "test": "vitest"
  },
  "dependencies": {
    "discord.js": "^14.18.0",
    "dotenv": "^16.4.7",
    "onnxruntime-node": "^1.21.0",
    "patchright": "^1.50.1",
    "pg": "^8.14.0",
    "sharp": "^0.33.5",
    "undici": "^7.5.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.13.10",
    "@types/pg": "^8.11.11",
    "prettier": "^3.5.3",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "vitest": "^3.0.8"
  },
  "packageManager": "yarn@4.7.0"
}
