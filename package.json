{
  "name": "discordbot",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start:dev": "ts-node --loader ts-node/esm src/discordBot.ts",
    "start": "node dist/discordBot.js",
    "deploy:dev": "ts-node --loader ts-node/esm src/deployCommands.ts",
    "deploy": "node dist/deployCommands.js",
    "init-db:dev": "ts-node --loader ts-node/esm src/initDb.ts",
    "init-db": "node dist/initDb.js",
    "clean": "rm -rf dist",
    "prebuild": "yarn clean",
    "test": "vitest"
  },
  "dependencies": {
    "@playwright/browser-chromium": "^1.50.1",
    "discord.js": "^14.7.1",
    "dotenv": "^16.4.7",
    "pg": "^8.13.1",
    "playwright": "^1.50.1",
    "undici": "^7.3.0"
  },
  "devDependencies": {
    "@types/node": "^22.13.1",
    "@types/pg": "^8.11.11",
    "prettier": "^3.4.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2",
    "vitest": "^3.0.7"
  }
}
