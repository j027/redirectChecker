{
  "name": "discordbot",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start:dev": "tsx src/discordBot.ts",
    "start": "node dist/discordBot.js",
    "deploy:dev": "tsx src/deployCommands.ts",
    "deploy": "node dist/deployCommands.js",
    "init-db:dev": "tsx src/initDb.ts",
    "init-db": "node dist/initDb.js",
    "clean": "rm -rf dist",
    "prebuild": "yarn clean",
    "test": "vitest"
  },
  "dependencies": {
    "@google-cloud/web-risk": "^5.3.1",
    "discord.js": "^14.25.1",
    "dotenv": "^17.2.3",
    "onnxruntime-node": "1.23.2",
    "patchright": "^1.57.0",
    "pg": "^8.16.3",
    "sharp": "^0.34.5",
    "undici": "^7.16.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@types/pg": "^8.15.6",
    "prettier": "^3.7.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "packageManager": "yarn@4.12.0"
}
